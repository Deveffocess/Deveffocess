<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <RelativeLayout
        android:id="@+id/rlTransporterx"
        android:layout_marginTop="5dp"
        android:padding="15dp"
        android:background="@drawable/white_round_shape"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <com.facebook.shimmer.ShimmerFrameLayout
            android:id="@+id/shimmerImagex"
            android:layout_width="80dp"
            android:layout_height="83dp">

            <com.livo.nuo.lib.roundImageView.RoundedImageView
                android:id="@+id/imgshimmerImagex"
                android:layout_width="80dp"
                android:layout_height="83dp"
                app:riv_corner_radius="8dp"
                android:scaleType="centerCrop"
                android:src="@drawable/grey_round_shape"/>

        </com.facebook.shimmer.ShimmerFrameLayout>

        <com.livo.nuo.lib.roundImageView.RoundedImageView
            android:id="@+id/imgProductImagex"
            android:layout_width="80dp"
            android:layout_height="83dp"
            app:riv_corner_radius="8dp"
            android:visibility="visible"
            android:scaleType="centerCrop"
            android:src="@drawable/grey_round_shape"/>


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvTitlex"
            android:layout_toEndOf="@+id/imgProductImagex"
            android:text="Title"
            android:layout_marginStart="18dp"
            android:fontFamily="@font/barlow_bold"
            android:textSize="13sp"
            android:textColor="@color/black"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvPricexx"
            android:layout_below="@+id/tvTitlex"
            android:layout_marginStart="18dp"
            android:layout_toEndOf="@+id/imgProductImagex"
            android:text="Price : 00 Kr"
            android:textAlignment="viewStart"
            android:textSize="12sp"
            android:fontFamily="@font/barlow_bold"
            android:textColor="@color/black"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>



        <RelativeLayout
            android:id="@+id/rlReievedOfferx"
            android:layout_toEndOf="@+id/imgProductImagex"
            android:layout_below="@+id/tvPricexx"
            android:layout_marginTop="7dp"
            android:layout_marginStart="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/white_fill_black_10_border"
            android:padding="7dp">



            <com.livo.nuo.lib.roundImageView.RoundedImageView
                android:id="@+id/imgTransporterImagex"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:visibility="gone"
                android:src="@drawable/grey_round_shape"
                app:riv_corner_radius="18dp"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTransporterNamex"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/imgTransporterImagex"
                android:fontFamily="@font/barlow_bold"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:text="Robin hood"
                android:visibility="gone"
                android:layout_marginStart="10dp"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTransporterStatusx"
                android:layout_below="@+id/tvTransporterNamex"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/imgTransporterImagex"
                android:fontFamily="@font/barlow_semibold"
                android:textColor="@color/livo_black_45_opacity"
                android:textSize="11sp"
                android:text="Driver Selected"
                android:visibility="gone"
                android:layout_marginStart="10dp"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvRecievedOfferx"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/barlow_bold"
                android:text="@string/received_offers_0"
                android:textAlignment="center"
                android:textColor="@color/livo_black_45_opacity"
                android:textSize="12sp"
                android:visibility="visible" />


        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imgProductImagex"
            android:layout_marginTop="25dp">

            <LinearLayout
                android:id="@+id/llChatCallx"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:weightSum="3">

                <RelativeLayout
                    android:id="@+id/rlChatx"
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_weight="1.1"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_fill_black_10_border"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    >

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgChatx"
                        android:src="@drawable/chat_grey"
                        android:layout_centerVertical="true"
                        android:layout_width="13dp"
                        android:layout_height="13dp"/>

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvChatx"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/imgChatx"
                        android:fontFamily="@font/barlow_bold"
                        android:text="@string/chat"
                        android:textAlignment="center"
                        android:textColor="@color/livo_black_30_opacity"
                        android:textSize="11sp" />


                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/rlCallx"
                    android:visibility="visible"
                    android:layout_weight="1.04"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_fill_black_10_border"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:layout_marginStart="12dp"
                    >

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgCallx"
                        android:src="@drawable/phone_grey"
                        android:layout_centerVertical="true"
                        android:layout_width="14dp"
                        android:layout_height="14dp"/>

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvCallx"
                        android:text="Call"
                        android:textAlignment="center"
                        android:textSize="11sp"
                        android:layout_toEndOf="@+id/imgCallx"
                        android:fontFamily="@font/barlow_bold"
                        android:textColor="@color/livo_black_30_opacity"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>


                </RelativeLayout>


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvDetailsx"
                    android:layout_marginStart="10dp"
                    android:background="@drawable/white_fill_black_10_border"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:text="@string/details"
                    android:lineHeight="15dp"
                    android:textSize="11sp"
                    android:fontFamily="@font/barlow_bold"
                    android:textColor="@color/livo_black_30_opacity"
                    android:layout_width="match_parent"
                    android:layout_weight="0.88"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"/>


            </LinearLayout>
            <LinearLayout
                android:id="@+id/llSelectx"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:weightSum="2">
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvSelectTransporterx"
                    android:visibility="visible"
                    android:layout_weight="0.8"
                    android:background="@drawable/white_fill_black_10_border"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:paddingTop="10dp"
                    android:lineHeight="15dp"
                    android:paddingBottom="10dp"
                    android:text="Select Transporter"
                    android:textSize="12sp"
                    android:fontFamily="@font/barlow_bold"
                    android:textColor="@color/livo_black_30_opacity"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>



                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvDetails1x"
                    android:layout_marginStart="10dp"
                    android:background="@drawable/white_fill_black_10_border"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:text="@string/details"
                    android:lineHeight="15dp"
                    android:textSize="11sp"
                    android:fontFamily="@font/barlow_bold"
                    android:textColor="@color/livo_black_30_opacity"
                    android:layout_width="match_parent"
                    android:layout_weight="1.2"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"/>


            </LinearLayout>

        </RelativeLayout>

    </RelativeLayout>


</layout>

